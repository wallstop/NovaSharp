<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <DefineConstants>$(DefineConstants);DOTNET_CORE;EMBEDTEST</DefineConstants>
        <AssemblyName>DotNetCoreTestRunner</AssemblyName>
        <OutputType>Exe</OutputType>
        <PackageId>DotNetCoreTestRunner</PackageId>
        <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
        <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\..\runtime\NovaSharp.Interpreter\NovaSharp.Interpreter.csproj" />
        <ProjectReference Include="..\..\..\debuggers\NovaSharp.VsCodeDebugger\NovaSharp.VsCodeDebugger.csproj" />
    </ItemGroup>
    <ItemGroup>
        <None Remove="moonsharp_tests.log" />
    </ItemGroup>
    <ItemGroup>
        <None Update="TestMore\**\*">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
    </ItemGroup>
</Project>
